{
  "name": "mmf-blog-vite-nuxt",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "build": "nuxi build",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "serve": "nuxi dev",
    "generate": "nuxi generate",
    "postinstall": "nuxi prepare",
    "start": "cross-env PORT=7222 node .output/server/index.mjs",
    "start:generate": "npx serve .output/public",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix",
    "prepare": "npx simple-git-hooks"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.1.4",
    "@iconify-json/carbon": "^1.2.13",
    "@iconify-json/twemoji": "^1.2.4",
    "@kangc/v-md-editor": "^2.3.18",
    "@lincy/eslint-config": "^5.8.1",
    "@lincy/stylelint-config": "^2.2.0",
    "@lincy/unocss-base-config": "^2.3.0",
    "@lincy/utils": "^0.6.6",
    "@nuxtjs/color-mode": "^3.5.2",
    "@pinia/nuxt": "^0.11.2",
    "@types/md5": "^2.3.5",
    "@types/prismjs": "^1.26.5",
    "@unocss/eslint-config": "^66.5.0",
    "@unocss/nuxt": "^66.5.0",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vue-macros/nuxt": "3.0.0-beta.22",
    "@vueuse/nuxt": "^13.8.0",
    "consola": "^3.4.2",
    "cross-env": "^10.0.0",
    "element-plus": "^2.11.1",
    "eslint": "^9.34.0",
    "eslint-plugin-format": "^1.0.1",
    "h3-proxy": "^1.13.0",
    "highlight.js": "^11.11.1",
    "md5": "^2.3.0",
    "nuxt": "^4.0.3",
    "pinia": "^3.0.3",
    "prismjs": "^1.30.0",
    "sass": "^1.91.0",
    "simple-git-hooks": "^2.13.1",
    "store2": "^2.14.4",
    "stylelint": "^16.23.1",
    "typescript": "^5.9.2",
    "vue-content-loader": "^2.0.1",
    "vue-loading-overlay": "^6.0.6",
    "vue-tsc": "^3.0.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup"
      ]
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
