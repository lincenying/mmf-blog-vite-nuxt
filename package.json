{
  "name": "mmf-blog-vite-nuxt",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.12.1",
  "scripts": {
    "build": "nuxi build",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "serve": "nuxi dev",
    "generate": "nuxi generate",
    "postinstall": "nuxi prepare",
    "start": "cross-env PORT=7222 node .output/server/index.mjs",
    "start:generate": "npx serve .output/public",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix",
    "prepare": "npx simple-git-hooks"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.18",
    "@lincy/utils": "^0.5.5",
    "md5": "^2.3.0",
    "prismjs": "^1.29.0",
    "store2": "^2.14.3",
    "vue-content-loader": "^2.0.1",
    "vue-loading-overlay": "^6.0.4"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.0.8",
    "@iconify-json/carbon": "^1.1.31",
    "@iconify-json/twemoji": "^1.1.15",
    "@lincy/base-config": "^1.2.1",
    "@lincy/eslint-config": "^4.4.0",
    "@lincy/stylelint-config": "^1.17.4",
    "@nuxtjs/color-mode": "^3.4.0",
    "@pinia/nuxt": "^0.5.1",
    "@types/md5": "^2.3.5",
    "@types/prismjs": "^1.26.3",
    "@unocss/eslint-config": "^0.59.2",
    "@unocss/nuxt": "^0.59.3",
    "@vite-pwa/nuxt": "^0.6.0",
    "@vue-macros/nuxt": "^1.9.30",
    "@vueuse/nuxt": "^10.9.0",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "element-plus": "^2.7.0",
    "eslint": "npm:eslint-ts-patch@8.57.0-0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-ts-patch": "8.57.0-0",
    "highlight.js": "^11.9.0",
    "nuxt": "^3.11.2",
    "pinia": "^2.1.7",
    "sass": "^1.75.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "vue-tsc": "^2.0.13"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix"
  }
}
